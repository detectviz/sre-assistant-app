{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Assistant",
  "id": "sre-assistant-app",
  "backend": true,
  "executable": "gpx_assistant",
  "info": {
    "keywords": ["app"],
    "description": "",
    "author": {
      "name": "Sre"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Overview",
      "path": "/a/%PLUGIN_ID%/overview",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "name": "Insights",
      "path": "/a/%PLUGIN_ID%/insight",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Incidents",
      "path": "/a/%PLUGIN_ID%/incident",
      "addToNav": true,
      "defaultNav": false
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=11.2.0",
    "plugins": []
  }
}
